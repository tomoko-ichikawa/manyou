<h1>ログイン</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>

  <%= f.label :email %>
  <%= f.email_field :email, id: 'session[email]' %>

  <%= f.label :password %>
  <%= f.password_field :password, id: 'session[password]' %>

  <%= f.submit "ログイン", id: "login" %>
<% end %>
